    "prettier": "^3.6.2",
    "simple-git-hooks": "^2.13.1",
    "swagger-typescript-api": "^13.2.9",
    "turbo": "^2.5.6",
    "typescript": "^5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@10.8.2",
  "workspaces": [
    "apps/*",
    "packages/*"
