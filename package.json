{
  "name": "hb-second-brain",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint --",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "task:visualize": "turbo run task:visualize",
    "check-types": "turbo run check-types",
